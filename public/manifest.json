{
  "short_name": "React App",
  "name": "PS Helper Chrome Extension",
  "version":"1.0",
  "manifest_version": 2, 
  "browser_action": { 
    "default_popup": "index.html", 
    "default_title": "Open the popup"
   }, 
  "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
  "permissions": [
    "background",
      "tabs",
      "<all_urls>"
    ],
  "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content.js"]
      }
    ]
}
